msgid "We are under attack!"
msgstr ""

#: dat/missions/minerva/pirate4.lua:286
msgid "We have been discovered!"
msgstr ""

#: dat/missions/minerva/pirate4.lua:291
msgid "#rMISSION FAILED! The interrogation ship was destroyed!"
msgstr ""

