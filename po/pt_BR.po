#: dat/missions/zalek/sciencegonewrong/00_sciwrong.lua:169
#: dat/missions/zalek/sciencegonewrong/00_sciwrong.lua:170
#: dat/missions/zalek/sciencegonewrong/02_sciwrong.lua:87
#: dat/missions/zalek/sciencegonewrong/02_sciwrong.lua:151
msgid "On the intercom"
msgstr ""

#: dat/missions/zalek/sciencegonewrong/00_sciwrong.lua:169
msgid ""
"\"We have reason to believe you are carrying controlled substances without a "
"proper license. Please stop your ship and prepare to be boarded.\""
msgstr ""

#: dat/missions/zalek/sciencegonewrong/00_sciwrong.lua:170
msgid "\"Stand down for inspection.\""
msgstr ""

