#: dat/missions/dvaered/frontier_war/fw00_escort.lua:263
#: dat/missions/dvaered/frontier_war/fw01_sabotage.lua:90
#: dat/missions/dvaered/frontier_war/fw03_sirius.lua:189
#: dat/missions/dvaered/frontier_war/fw03_sirius.lua:192
#: dat/missions/dvaered/frontier_war/fw03_sirius.lua:195
